{
    "name": "Visual Enhancement",
    "version": "1.0",
    "description": "For the Visually Enhanced",
    "permissions": ["storage", "activeTab", "https://ajax.googleapis.com/",
                    "https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js"],
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "css":["betty_white.css"]   
        }
    ],
    "page_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "web_accessible_resources": ["fonts/*"]
  }